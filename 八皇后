#include<iostream>
using namespace std;

int q[8] = { -1,-1,-1,-1,-1,-1,-1,-1 }; //存每个皇后的列位置

int queen(int row,int recorde);
int judge(int i, int j);

int main()
{
	int row(1);
	int firstsite(0);
	cin >> firstsite;
	q[0] = firstsite;
	int c(0);
	queen(row,c);
	for (int i(0); i < 8; i++)
	{
		cout << q[i];
	}
	cout << endl;
	return 0;
}

int queen(int row,int c)
{
	int recorde(0);
	for ( ; c < 8; c++)
	{
		if (judge(row, c) == 1)
		{
			q[row] = c + 1;
			recorde = c + 1;
			if (row == 7)
			{
				for (int i(0); i < 8; i++)
				{
					cout << q[i];
				}
				cout << endl;
				return 1;
			}
			return queen(row + 1,0);
		}
	}
	return queen(--row, recorde);
}

int judge(int i, int j)
{
	for (int c(0); c<i; c++)
	{
		if (j == (q[c] - 1) || (i - j) == (c - (q[c] - 1)) || (i + j) == (c + (q[c] - 1)))
		{
			return 0;
		}
	}
	return 1;
}
